// launch.json contains information needed for the debugger to attach to  processes
// See https://code.visualstudio.com/Docs/editor/debugging#_launch-configurations
// for documentation about how to create and manage configurations
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run on Clour Run",
      "type": "cloudcode.cloudrun",
      "request": "launch",
      "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
      "watch": true,
      "cleanUp": true,
      "portForward": true
    },
    {
      "name": "Debug on Cloud Run",
      "type": "cloudcode.cloudrun",
      "request": "attach",
      "podSelector": {
        "app": "dotnet-cloud-run-hello-world",
      },
      "localRoot": "${workspaceFolder}/src",
      "language": "NETCore",
      "remoteRoot": "/src",
      "debugPort": 9229
    }
  ]
}
